		{# Run `composer require symfony/webpack-encore-bundle` to start using Symfony UX #}

		{# CSS Local Bootstrap#}
		<link href="/css/bootstrap.css" rel="stylesheet">

        {# Favicon #}
        <link rel="apple-touch-icon" sizes="180x180" href="/apple-touch-icon.png">
